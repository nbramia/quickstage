{
  "name": "@quickstage/web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && cp -r functions dist/ && cp public/test-routing.html dist/ && cp public/_worker.js dist/ && cp public/_routes.json dist/",
    "preview": "vite preview --port 5173",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc -b --pretty false",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:critical": "vitest run --reporter=verbose src/test/components/Landing.test.tsx src/test/components/Login.test.tsx src/test/components/Dashboard.test.tsx",
    "predeploy": "pnpm test:critical && pnpm build",
    "run-tests": "tsx src/test/run-tests.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.5",
    "typescript": "^5.4.3",
    "vite": "^5.2.8",
    "vitest": "^3.2.4"
  }
}
